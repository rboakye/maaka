<% @kasas.each do |kasa|%>
    <% if kasa.momentable_type == 'Post' %>
        <%= render partial: 'posts/kasa', :locals => {:kasa => find_momentable(kasa.momentable_id, 'Post'), user: @current_user, :comment => comment} %>
    <% elsif kasa.momentable_type == 'Image'  %>
        <%= render partial: 'posts/my_image', :locals => {:image => find_momentable(kasa.momentable_id, 'Image'), user: @current_user} %>
    <% else %>

    <% end %>
<% end %>